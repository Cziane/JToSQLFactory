{
	"name": "Migrate Json To Cosmo",
	"properties": {
		"activities": [
			{
				"name": "Copy Restaurant From Json to Cosmo",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['address']['building']"
								}
							},
							{
								"source": {
									"path": "$['address']['coord'][0]"
								}
							},
							{
								"source": {
									"path": "$['address']['street']"
								}
							},
							{
								"source": {
									"path": "$['address']['zipcode']"
								}
							},
							{
								"source": {
									"path": "$['borough']"
								}
							},
							{
								"source": {
									"path": "$['cuisine']"
								}
							},
							{
								"source": {
									"path": "$['grades'][0]['date']['$date']"
								}
							},
							{
								"source": {
									"path": "$['grades'][0]['grade']"
								}
							},
							{
								"source": {
									"path": "$['grades'][0]['score']"
								}
							},
							{
								"source": {
									"path": "$['name']"
								}
							},
							{
								"source": {
									"path": "$['restaurant_id']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "RestaurantFile",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "RestaurantCosmo",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Cosmo To SQL",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "Copy Restaurant From Json to Cosmo",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "1.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "Cosmo To SQL",
						"type": "DataFlowReference"
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			},
			{
				"name": "COSMO to SQL Grades",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "Cosmo To SQL",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "1.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "COSMO to SQL Grades",
						"type": "DataFlowReference"
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			}
		],
		"annotations": []
	}
}