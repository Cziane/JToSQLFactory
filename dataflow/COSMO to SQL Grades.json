{
	"name": "COSMO to SQL Grades",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "RestaurantCosmo",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: 'SELECT \\n     StringToNumber(c.restaurant_id) as id,\\n    m.grade as grade,\\n    StringToNumber(m.score as score),\\n    m.date[\\'$date\\'] as date_create\\nFROM\\n     c join m in c.grades',",
				"     format: 'documentQuery',",
				"     systemColumns: false) ~> source1"
			]
		}
	}
}